package com.bory.company.dao;

import java.util.List;
import java.util.Map;

import com.bory.company.domain.EmployeeBean;

public interface CompanyDAO {
	/**
	 * DTO - 가변적인, 값을 주고받는 경우
	 * VO - 값이 영속적인 경우, 변하지 않는 경우
	 * */
	public int insert(EmployeeBean employeeBean);
	public int update(EmployeeBean employeeBean);
	public List<EmployeeBean> find();
	public List<EmployeeBean> readSomeEmployee(Map<String, String> readOption);
	public List<EmployeeBean> findOne(String readOption);
}
