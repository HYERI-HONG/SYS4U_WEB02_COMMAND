package com.bory.company.dao;

import java.util.List;
import java.util.Map;

import com.bory.company.dto.EmployeeBean;

public interface CompanyDAO {
	
	public int insert(EmployeeBean employeeBean);
	public int update(EmployeeBean employeeBean);
	public List<EmployeeBean> findAll();
	public List<EmployeeBean> findSome(Map<String, String> findOption);
	public List<EmployeeBean> findOne(String readOption);
	public int count();
}
